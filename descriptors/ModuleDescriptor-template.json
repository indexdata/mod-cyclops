{
  "id": "@artifactId@-@version@",
  "name": "CYCLOPS module",
  "provides": [
    {
      "id": "cyclops",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/tags",
          "permissionsRequired": [
            "cyclops.tags.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/tags",
          "permissionsRequired": [
            "cyclops.tags.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/filters",
          "permissionsRequired": [
            "cyclops.filters.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/filters",
          "permissionsRequired": [
            "cyclops.filters.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/sets",
          "permissionsRequired": [
            "cyclops.sets.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets",
          "permissionsRequired": [
            "cyclops.sets.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/sets/{setName}",
          "permissionsRequired": [
            "cyclops.sets.retrieve"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets/{setName}",
          "permissionsRequired": [
            "cyclops.sets.modify"
          ],
          "permissionsDesired": [
            "cyclops.sets.add-objects",
            "cyclops.sets.remove-objects"
	  ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets/{setName}/tags/{tagName}",
          "permissionsRequired": [
            "cyclops.sets.modify-tags"
          ],
          "permissionsDesired": [
            "cyclops.sets.add-tags",
            "cyclops.sets.remove-tags"
	  ]
        }
      ]
    }
  ],
  "requires": [],
  "permissionSets": [
    {
      "permissionName": "cyclops.tags.read",
      "displayName": "CYCLOPS: Read list of tags"
    },
    {
      "permissionName": "cyclops.tags.create",
      "displayName": "CYCLOPS: Create tag"
    },
    {
      "permissionName": "cyclops.filters.read",
      "displayName": "CYCLOPS: Read list of filters"
    },
    {
      "permissionName": "cyclops.filters.create",
      "displayName": "CYCLOPS: Create filter"
    },
    {
      "permissionName": "cyclops.sets.read",
      "displayName": "CYCLOPS: Read list of sets"
    },
    {
      "permissionName": "cyclops.sets.create",
      "displayName": "CYCLOPS: Create set"
    },
    {
      "permissionName": "cyclops.sets.retrieve",
      "displayName": "CYCLOPS: Retrieve objects from set"
    },
    {
      "permissionName": "cyclops.sets.modify",
      "displayName": "CYCLOPS: Add/remove objects to/from set"
    },
    {
      "permissionName": "cyclops.sets.add-objects",
      "displayName": "CYCLOPS: Add objects to set (UNIMPLEMENTED)",
      "subPermissions": ["cyclops.sets.modify"]
    },
    {
      "permissionName": "cyclops.sets.remove-objects",
      "displayName": "CYCLOPS: Remove objects from set (UNIMPLEMENTED)",
      "subPermissions": ["cyclops.sets.modify"]
    },
    {
      "permissionName": "cyclops.sets.modify-tags",
      "displayName": "CYCLOPS: Add/remove tags from objects in a set"
    },
    {
      "permissionName": "cyclops.sets.add-tags",
      "displayName": "CYCLOPS: Add tags to objects in a set (UNIMPLEMENTED)",
      "subPermissions": ["cyclops.sets.modify-tags"]
    },
    {
      "permissionName": "cyclops.sets.remove-tags",
      "displayName": "CYCLOPS: Remove tags from objects in a set (UNIMPLEMENTED)",
      "subPermissions": ["cyclops.sets.modify-tags"]
    }
  ],
  "launchDescriptor": {
    "dockerImage": "@artifactId@:@version@",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 100000000,
        "PortBindings": {
          "12370/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "env": [
      {
        "name": "LOGCAT",
        "value": "listen,path,op,status,db,error"
      },
      {
        "name": "MOD_CYCLOPS_QUERY_TIMEOUT",
        "value": "300"
      }
    ]
  }
}
