{
  "id": "@artifactId@-@version@",
  "name": "CYCLOPS module",
  "provides": [
    {
      "id": "cyclops",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/tags",
          "permissionsRequired": [
            "cyclops.tags.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/tags",
          "permissionsRequired": [
            "cyclops.tags.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/filters",
          "permissionsRequired": [
            "cyclops.filters.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/filters",
          "permissionsRequired": [
            "cyclops.filters.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/sets",
          "permissionsRequired": [
            "cyclops.sets.read"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets",
          "permissionsRequired": [
            "cyclops.sets.create"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/cyclops/sets/{setName}",
          "permissionsRequired": [
            "cyclops.sets.retrieve"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets/{setName}",
          "permissionsRequired": [
            "cyclops.sets.modify"
          ],
          "permissionsDesired": [
            "cyclops.sets.add-objects",
            "cyclops.sets.remove-objects"
	  ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/cyclops/sets/{setName}/tags/{tagName}",
          "permissionsRequired": [
            "cyclops.sets.modify-tags"
          ],
          "permissionsDesired": [
            "cyclops.sets.add-tags",
            "cyclops.sets.remove-tags"
	  ]
        }
      ]
    }
  ],
  "requires": [],
  "permissionSets": [
    {
      "displayName": "Read CYCLOPS data",
      "permissionName": "cyclops.read",
      "subPermissions": []
    },
    {
      "displayName": "Write CYCLOPS data",
      "permissionName": "cyclops.write",
      "subPermissions": []
    }
  ],
  "launchDescriptor": {
    "dockerImage": "@artifactId@:@version@",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 100000000,
        "PortBindings": {
          "12370/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "env": [
      {
        "name": "LOGCAT",
        "value": "listen,path,op,status,db,error"
      },
      {
        "name": "MOD_CYCLOPS_QUERY_TIMEOUT",
        "value": "300"
      }
    ]
  }
}
